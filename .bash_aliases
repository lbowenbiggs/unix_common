#common mistypes
alias cd-="cd -"
alias sl="ls"
alias maek="make"

# git
alias gitl="git log --oneline -n10"
alias gitst="git status"
alias gita="git add ."
alias gitgraph="git log --online --decorate --all --graph"
alias gitb="git branch -vv"

# tar
alias untar="tar -zxvf"
alias targz="tar -czvf"

# ls
alias ll="ls -alF"
alias la="ls -A"
alias l="ls -CF"
